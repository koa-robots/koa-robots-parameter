{
  "name": "koa-robots-parameter",
  "version": "0.0.1",
  "description": "parse request parameter",
  "author": "寒飞紫",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.3"
  },
  "main": "./dist/index",
  "dependencies": {
    "koa-body": "^1.4.0",
    "koa-qs": "^2.0.0",
    "xss": "^0.2.10"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-plugin-transform-es2015-destructuring": "^6.4.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "babel-plugin-transform-es2015-spread": "^6.4.0",
    "koa": "^1.1.2",
    "should": "8.0.2",
    "supertest": "1.1.0",
    "urllib": "^2.7.0"
  },
  "babel": {
    "plugins": [
      "transform-es2015-spread",
      "transform-es2015-parameters",
      "transform-es2015-destructuring",
      "transform-es2015-modules-commonjs"
    ]
  },
  "scripts": {
    "compile": "babel src -d dist",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --reporter spec --require should --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koa-robots/koa-robots-static.git"
  }
}
